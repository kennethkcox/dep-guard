{
  "name": "depguard",
  "version": "2.0.0",
  "description": "Advanced dependency vulnerability scanner with reachability analysis",
  "main": "src/index.js",
  "bin": {
    "depguard": "./bin/depguard.js"
  },
  "scripts": {
    "start": "node bin/depguard.js",
    "test": "jest",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src/**/*.js",
    "precommit": "npm test",
    "prepare": "node -e \"try { require('fs').mkdirSync('.husky') } catch(e) {}\""
  },
  "keywords": [
    "security",
    "vulnerability",
    "dependency",
    "reachability",
    "static-analysis",
    "supply-chain-security"
  ],
  "author": "DepGuard Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.26.5",
    "@babel/traverse": "^7.26.5",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "chalk": "^4.1.2",
    "commander": "^14.0.3",
    "glob": "^11.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "jest": "^30.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}