<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <!-- CVE-2022-29117, CVE-2022-29145 - Denial of Service -->
    <PackageReference Include="System.Text.Encodings.Web" Version="4.7.1" />

    <!-- CVE-2021-26701 - Remote Code Execution -->
    <PackageReference Include="System.Text.Encodings.Web" Version="4.5.0" />

    <!-- CVE-2022-34716 - Information Disclosure -->
    <PackageReference Include="System.Security.Cryptography.Xml" Version="4.7.0" />

    <!-- CVE-2021-34485 - Information Disclosure -->
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.1.18" />

    <!-- CVE-2022-41032 - Privilege Escalation -->
    <PackageReference Include="NuGet.Commands" Version="5.11.5" />

    <!-- Newtonsoft.Json - older versions have known vulnerabilities -->
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />

    <!-- log4net - CVE-2018-1285 -->
    <PackageReference Include="log4net" Version="2.0.10" />

    <!-- ImageSharp - CVE-2022-24741 -->
    <PackageReference Include="SixLabors.ImageSharp" Version="2.1.11" />
  </ItemGroup>

</Project>
